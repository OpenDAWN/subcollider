{
  "name": "subcollider",
  "description": "JavaScript utility library that provides like SuperCollider functions",
  "version": "0.2.0",
  "author": "Nao Yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/subcollider/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.1.11",
    "babel-eslint": "^3.0.1",
    "browserify": "^9.0.8",
    "coveralls": "^2.11.2",
    "eslint": "^0.19.0",
    "espower-babel": "^2.1.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.4",
    "power-assert": "^0.11.0",
    "uglify-js": "^2.4.20"
  },
  "documentIndex": [
    "src/sc.js",
    "src/number.js",
    "src/array.js",
    "src/random/random.js",
    "src/config.js"
  ],
  "files": [
    "package.json",
    "README.md",
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/subcollider/",
  "keywords": [
    "util",
    "functional",
    "music",
    "supercollider"
  ],
  "licenses": "MIT",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/subcollider.git"
  },
  "scripts": {
    "build": "npm run build-to5 && npm run build-browser && npm run build-minify",
    "build-browser": "browserify lib/index.js --standalone sc --outfile build/subcollider.js",
    "build-minify": "uglifyjs build/subcollider.js -o build/subcollider.min.js",
    "build-to5": "babel src --out-dir lib",
    "cover": "babel-node node_modules/.bin/isparta cover --report text --report html --report lcov node_modules/.bin/_mocha",
    "lint": "eslint src test",
    "test": "mocha --require espower-babel/guess",
    "travis": "npm run lint && npm run cover"
  }
}
